<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<b:Hero ui:field="heroUnit">
			<b:PageHeader>Welcome!</b:PageHeader>
			<b:Paragraph ui:field="message"></b:Paragraph>
			<b:ButtonGroup styleName="pull-right">
				<b:Button ui:field="button" icon="COG" iconColor="WHITE" type="PRIMARY" size="LARGE">Setup</b:Button>
			</b:ButtonGroup>
		</b:Hero>
		<b:Well ui:field="well" visible="false"></b:Well>
	</g:HTMLPanel>
</ui:UiBinder> 