<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:style>
		.center {
			margin-left: auto;
			margin-right: auto;
		}
	</ui:style>
	<b:ButtonToolbar>
		<b:ButtonGroup>
			<b:Button icon="OFF">All units off</b:Button>
			<b:Button icon="EYE_CLOSE">All lights off</b:Button>
			<b:Button icon="EYE_OPEN">All lights on</b:Button>
		</b:ButtonGroup>
		<b:ButtonGroup addStyleNames="{style.center}">
			<b:Button icon="TH_LIST">Show list</b:Button>
			<b:Button icon="TH">Show tiled</b:Button>
		</b:ButtonGroup>
		<b:ButtonGroup addStyleNames="pull-right">
			<b:Button ui:field="addNewUnitBtn" icon="PLUS_SIGN">Add new unit</b:Button>
		</b:ButtonGroup>
	</b:ButtonToolbar>
</ui:UiBinder>
