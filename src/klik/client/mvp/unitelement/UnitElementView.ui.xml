<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:style>
		.label {
			margin-left: 25px;
		}
	</ui:style>
	<b:Well>
		<b:Button ui:field="extendButton" icon="CHEVRON_DOWN" size="LARGE"></b:Button>
		<g:InlineLabel ui:field="nameLabel" styleName="{style.label}"/>
		<g:HTMLPanel tag="span" styleName="pull-right clearfix">
			<g:HTMLPanel tag="span" ui:field="widgetPanel">
				hello
			</g:HTMLPanel>
			<b:Button ui:field="onOffButton" icon="OFF" iconColor="WHITE" size="LARGE"></b:Button>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="invisiblePanel" visible="false">
			<hr />
			<b:ProgressBar percent="90" />
		</g:HTMLPanel>
	</b:Well>
</ui:UiBinder>
