<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:with type="klik.client.NameTokens" field="nameTokens"></ui:with>
	<ui:with type="klik.client.resources.Resources" field="resources"></ui:with>
	<ui:style>
		.push {
			padding-top: 40px;
		}
	</ui:style>
	<g:HTMLPanel>

		<b:Navbar position="TOP">
			<b:Brand>klik klik klik</b:Brand>
			<b:Nav>
				<b:NavLink href="#{nameTokens.getHome}">Home</b:NavLink>
				<b:NavLink href="#{nameTokens.getAdmin}" styleName="">Admin</b:NavLink>
				<b:NavLink href="#{nameTokens.getError}" styleName="">Error</b:NavLink>
			</b:Nav>
		</b:Navbar>

		<g:HTMLPanel styleName="container">
			<g:HTMLPanel ui:field="alertPanel" styleName="{style.push}">
				<b:Alert ui:field="alert" animation="true" visible="true">This is a test!</b:Alert>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="contentPanel"></g:HTMLPanel>
		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder>